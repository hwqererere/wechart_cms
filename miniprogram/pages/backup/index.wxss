.index{float:left;width:100%;}

.index .li{float:left;width:50%;color:#ffffff;height:240rpx;line-height:240rpx;text-align:center;}
.index .li:nth-child(1){background-color:#908711}
.index .li:nth-child(2){background-color:#880088}
.index .li:nth-child(3){background-color:#ff00ff}
.index .li:nth-child(4){background-color:#ff77ff}


.file{float:left;width:100%;}
.file .list{float:left;width:96%;margin-left:2%;height:200rpx;border-bottom:1rpx solid #909090}
.file .list checkbox{float:left;margin-top:70rpx;}
.file .list .name{float:left;width:calc(100% - 260rpx);height:200rpx;line-height:200rpx;}
.file .list image{float:left;width:180rpx;height:180rpx;margin-top:10rpx;}
.file .list video{float:left;width:180rpx;height:180rpx;margin-top:10rpx;}
.file .foot{float:left;width:100%;height:100rpx;background-color:#ffffff}
.file .foot .fixedlay{position:fixed;bottom:0rpx;left:0rpx;width:100%;height:100rpx;border-top:1rpx solid #6e6e6e}
.file .foot .fixedlay button{float:left;margin:10rpx 10rpx;}
.file .foot .fixedlay button:first-child{margin-left:20rpx;}